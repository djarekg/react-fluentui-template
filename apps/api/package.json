{
  "name": "@[PROJECT_NAME_SHORT]/api",
  "private": false,
  "version": "1.0.0",
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "pnpm": "10.17.1"
  },
  "type": "module",
  "imports": {
    "#app/*": "./src/*",
    "#app/generated/prisma/client/*": "./src/generated/prisma/client/*",
    "#prisma/seed/*": "./prisma/seed/*"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.ts"
    }
  },
  "scripts": {
    "db:clean": "pnpx prisma migrate reset --force",
    "db:seed": "dotenv -e .env -- node --loader ts-node/esm prisma/seed/seed.ts --tsconfig=tsconfig.json",
    "db": "pnpm run db:clean && pnpx prisma generate && pnpx prisma db push && pnpm run db:seed",
    "build": "",
    "dev": "node --loader ts-node/esm --watch --env-file=.env --experimental-default-config-file --no-warnings=ExperimentalWarning src/server.ts"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^14.0.0",
    "@prisma/client": "^6.17.1",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "koa": "^3.0.3",
    "koa-body": "^6.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@prisma/adapter-pg": "^6.17.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/koa": "^3.0.0",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^24.8.1",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
